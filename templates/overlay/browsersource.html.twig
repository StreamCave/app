{% extends "base_overlay.html.twig" %}


{% block body %}
	<pre>
	{{ dump(overlays) }}
</pre>
	{% for overlay in overlays %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "cam_heros" or overlay.widgetIdBeta and overlay.widgetIdBeta == "cam_heros" %}
			{{ include('overlay/widgets/cams/five.html.twig') }}
		{% endif %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "topbar" or overlay.widgetIdBeta and overlay.widgetIdBeta == "topbar" %}
			{{ include('overlay/widgets/topbar/heros.html.twig') }}
		{% endif %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "versus" or overlay.widgetIdBeta and overlay.widgetIdBeta == "versus" %}
			{{ include('overlay/widgets/versus/v1.html.twig') }}
		{% endif %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "popup_text" or overlay.widgetIdBeta and overlay.widgetIdBeta == "popup_text" %}
			{{ include('overlay/widgets/popup/small_txt.html.twig') }}
		{% endif %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "next" or overlay.widgetIdBeta and overlay.widgetIdBeta == "next" %}
			{{ include('overlay/widgets/next/v1.html.twig') }}
		{% endif %}
		{% if overlay.widgetIdAlpha and overlay.widgetIdAlpha == "tweets" or overlay.widgetIdBeta and overlay.widgetIdBeta == "tweets" %}
			{{ include('overlay/widgets/tweets/index.html.twig') }}
		{% endif %}
	{% endfor %}
{% endblock %}
